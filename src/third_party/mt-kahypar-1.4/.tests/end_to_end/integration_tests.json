                   { "partitioner": "Mt-KaHyPar-Q",
                     "fixed_vertices": true,
                     "parameters": ["--num-vcycles=1"] } ] },
      { "name": "Mt-KaHyPar (n-Level Code) with Fixed Vertices",
              "instances": [ "tests/instances/ibm01.hgr",
