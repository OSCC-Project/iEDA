Grammar

    0 $accept: file $end

    1 file: modules

    2 modules: %empty
    3        | modules module

    4 @1: %empty

    5 module_begin: MODULE @1

    6 module: module_begin ID ';' stmts ENDMODULE
    7       | module_begin ID '(' ')' ';' stmts ENDMODULE
    8       | module_begin ID '(' port_list ')' ';' stmts ENDMODULE
    9       | module_begin ID '(' port_dcls ')' ';' stmts ENDMODULE

   10 port_list: port
   11          | port_list ',' port

   12 port: port_expr
   13     | '.' ID '(' ')'
   14     | '.' ID '(' port_expr ')'

   15 port_expr: port_ref
   16          | '{' port_refs '}'

   17 port_refs: port_ref
   18          | port_refs ',' port_ref

   19 port_ref: net_scalar
   20         | net_bit_select
   21         | net_part_select

   22 port_dcls: port_dcl
   23          | port_dcls ',' port_dcl
   24          | port_dcls ',' dcl_arg

   25 @2: %empty

   26 port_dcl: port_dcl_type @2 dcl_arg

   27 @3: %empty

   28 port_dcl: port_dcl_type @3 '[' INT ':' INT ']' dcl_arg

   29 port_dcl_type: INPUT
   30              | INPUT WIRE
   31              | INOUT
   32              | INOUT REG
   33              | INOUT WIRE
   34              | OUTPUT
   35              | OUTPUT WIRE
   36              | OUTPUT REG

   37 stmts: %empty
   38      | stmts stmt
   39      | stmts stmt_seq

   40 stmt: parameter
   41     | defparam
   42     | declaration
   43     | instance
   44     | error ';'

   45 stmt_seq: continuous_assign

   46 parameter: PARAMETER parameter_dcls ';'
   47          | PARAMETER '[' INT ':' INT ']' parameter_dcls ';'

   48 parameter_dcls: parameter_dcl
   49               | parameter_dcls ',' parameter_dcl

   50 parameter_dcl: ID '=' parameter_expr
   51              | ID '=' STRING

   52 parameter_expr: ID
   53               | '`' ID
   54               | CONSTANT
   55               | INT
   56               | '-' parameter_expr
   57               | parameter_expr '+' parameter_expr
   58               | parameter_expr '-' parameter_expr
   59               | parameter_expr '*' parameter_expr
   60               | parameter_expr '/' parameter_expr
   61               | '(' parameter_expr ')'

   62 defparam: DEFPARAM param_values ';'

   63 param_values: param_value
   64             | param_values ',' param_value

   65 param_value: ID '=' parameter_expr
   66            | ID '=' STRING

   67 @4: %empty

   68 declaration: dcl_type @4 dcl_args ';'

   69 @5: %empty

   70 declaration: dcl_type @5 '[' INT ':' INT ']' dcl_args ';'

   71 dcl_type: INPUT
   72         | INOUT
   73         | OUTPUT
   74         | SUPPLY0
   75         | SUPPLY1
   76         | TRI
   77         | WAND
   78         | WIRE
   79         | WOR

   80 dcl_args: dcl_arg
   81         | dcl_args ',' dcl_arg

   82 dcl_arg: ID
   83        | net_assignment

   84 continuous_assign: ASSIGN net_assignments ';'

   85 net_assignments: net_assignment
   86                | net_assignments ',' net_assignment

   87 @6: %empty

   88 net_assignment: net_assign_lhs @6 '=' net_expr

   89 net_assign_lhs: net_named
   90               | net_expr_concat

   91 @7: %empty

   92 instance: ID @7 ID '(' inst_pins ')' ';'

   93 @8: %empty

   94 instance: ID @8 parameter_values ID '(' inst_pins ')' ';'

   95 parameter_values: '#' '(' parameter_exprs ')'

   96 parameter_exprs: parameter_expr
   97                | '{' parameter_exprs '}'
   98                | parameter_exprs ',' parameter_expr

   99 inst_pins: %empty
  100          | inst_ordered_pins
  101          | inst_named_pins

  102 inst_ordered_pins: net_expr
  103                  | inst_ordered_pins ',' net_expr

  104 inst_named_pins: inst_named_pin
  105                | inst_named_pins ',' inst_named_pin

  106 inst_named_pin: '.' ID '(' ')'
  107               | '.' ID '(' ID ')'
  108               | '.' ID '(' ID '[' INT ']' ')'
  109               | '.' ID '(' named_pin_net_expr ')'
  110               | '.' ID '[' INT ']' '(' ')'
  111               | '.' ID '[' INT ']' '(' net_expr ')'
  112               | '.' ID '[' INT ':' INT ']' '(' ')'
  113               | '.' ID '[' INT ':' INT ']' '(' net_expr ')'

  114 named_pin_net_expr: net_part_select
  115                   | net_constant
  116                   | net_expr_concat

  117 net_named: net_scalar
  118          | net_bit_select
  119          | net_part_select

  120 net_scalar: ID

  121 net_bit_select: ID '[' INT ']'

  122 net_part_select: ID '[' INT ':' INT ']'

  123 net_constant: CONSTANT

  124 net_expr_concat: '{' net_exprs '}'

  125 net_exprs: net_expr
  126          | net_exprs ',' net_expr

  127 net_expr: net_scalar
  128         | net_bit_select
  129         | net_part_select
  130         | net_constant
  131         | net_expr_concat


